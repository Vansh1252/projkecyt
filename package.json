{
  "name": "NEW FOLDER (2)",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 500",
    "lint:fix": "eslint . --fix --max-warnings 500",
    "lint:strict": "eslint . --max-warnings 500 --report-unused-disable-directives",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:strict": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:strict && npm run format:check",
    "sonar": "sonar-scanner",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "clean": "rm -rf dist build coverage .eslintcache",
    "audit:security": "npm audit --audit-level high",
    "audit:deps": "npm outdated",
    "cz": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix --max-warnings 0 --no-ignore --no-warn-ignored",
      "prettier --write"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix --max-warnings 0 --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,css,md,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.10.1",
    "@sparticuz/chromium-min": "^141.0.0",
    "@supabase/supabase-js": "^2.81.0",
    "@types/pdfkit": "^0.17.3",
    "@untitledui/icons": "^0.0.19",
    "axios": "^1.13.2",
    "blob-stream": "^0.1.3",
    "clsx": "^2.1.1",
    "cz": "^1.8.2",
    "formik": "^2.4.9",
    "next": "15.4.1",
    "openai": "^6.9.0",
    "puppeteer-core": "^10.1.0",
    "react": "19.1.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "recharts": "^3.4.1",
    "resend": "^6.5.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.1",
    "typescript-eslint": "^8.37.0",
    "yap": "^0.0.2",
    "yup": "^1.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "autoprefixer": "^10.4.21",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.31.0",
    "sonarqube-scanner": "^4.3.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
